<%- include('partials/header') %>
<div class="main w-full h-screen ">
    <div class="cards flex flex-wrap gap-10  ">
        <% user.posts.forEach(function(elem){%>
            <div class="card w-72      ">
                <div class=" w-72 h-40 bg-red-500 ml-4 mt-4 border-solid border-2 border-sky-500 rounded-md overflow-hidden ">
                    <img class="w-full h-full object-cover  " src="/images/uploads/<%= elem.postimage %>" alt="">
                </div>
                <h1 class="text-zinc-800 text-xl ml-4 py-0 "><%= elem.caption %></h1>
                <p class="text-zinc-500  ml-4 py-0 ">@<%= user.username %></p>
        </div>

            
            
        <%}) %>
        
</div>

   
        
</div>
<%- include('partials/footer') %>